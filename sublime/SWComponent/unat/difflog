-void DirectSegmentIterator::decomposeArray(swInt subSegNum, swInt subSegDataLimit,
-			swInt* dataWeights, swInt size)
-{
-	swInt64 weightSum = 0;
-	for(swInt ivert = 0; ivert<size; ivert++)
